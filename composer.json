        },

        {
            "name": "Igor Goroshit",
            "email": "igor@webt.co.il"
        }

    ],

    "require": {
        "php":                  "~7.4|~8.0|~8.1",
        "illuminate/support":   "~7.0|~8.0|~9.0|~10.0",
        "axy/sourcemap":        "0.1.5",
